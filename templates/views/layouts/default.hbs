    <title>Tiffany</title>

		<link rel="shortcut icon" href="/favicon.ico" type="image/x-icon">

    <!-- Bootstrap core CSS -->
    <link href="/styles/bootstrap/bootstrap.min.css" rel="stylesheet">

    <!-- Owl Carousel -->
		<link type="text/css" rel="stylesheet" href="/styles/owl/owl.carousel.css" />
